<p style="color: green"><%= notice %></p>

<h1>Licitaciones</h1>

<div id="licitacions">

  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Nombre proyecto</th>
        <th scope="col">Descripcion</th>
        <th scope="col">Presupuesto</th>
        <th scope="col">Duracion en meses</th>
        <th scope="col">Fecha de inicio</th>
        <th scope="col">Fk region</th>
        <th scope="col">estado licitacion</th>
        <th scope="col">Acci√≥n</th>       
      </tr>
    </thead>
    <tbody>
      <% @licitacions.each do |licitacion| %>
        <tr>
          <td><%= licitacion.nombre_proyecto %></td>
          <td><%= licitacion.descripcion %></td>
          <td><%= licitacion.presupuesto %></td>
          <td><%= licitacion.duracion_en_meses %></td>
          <td><%= licitacion.fecha_de_inicio %></td>
          <td><%= licitacion.fk_region %></td>
          <td><%= licitacion.fk_estado_licitacion %></td>
          <td><%= link_to "Show", licitacion %></td>
        </tr>
      <% end %>
    </tbody>
  </table> 

</div>

<%= link_to "New licitacion", new_licitacion_path %>
