<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<br>

<% if user_signed_in? %>
    <p>Bienvenido: <%= current_user.name %> 
    <p>Usuario: <%= current_user.email %></p>
    <p>Id: <%= current_user.id %></p>
    <br>

    <p><%= link_to "Editar datos", edit_user_registration_path %></p>

    <%= link_to "Cerrar Sesion", destroy_user_session_path %>

<% else %>

    <p>Debe iniciar sesión</p>
    <%= link_to "Iniciar sesión", new_user_session_path %>

<% end %>